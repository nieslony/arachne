<?xml version="1.0" encoding="UTF-8"?>
<!--     allowLinking="true" antiJARLocking="true"
-->
<Context
    path="/arachne"
    allowLinking="true">
    <Resources allowLinking="true">
        <PostResources base="@@JAR_DIR@@/commons-digester.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/commons-digester.jar"/>
        <PostResources base="@@JAR_DIR@@/commons-logging.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/commons-logging.jar"/>
        <PostResources base="@@JAR_DIR@@/commons-beanutils.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/commons-beanutils.jar"/>
        <PostResources base="@@JAR_DIR@@/commons-collections.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/commons-collections.jar"/>

        <PostResources base="@@JAR_DIR@@/bcprov.jar"
                className="org.apache.catalina.webresources.FileResourceSet"
                webAppMount="/WEB-INF/lib/bcprov.jar"/>
        <PostResources base="@@JAR_DIR@@/bcpkix.jar"
                className="org.apache.catalina.webresources.FileResourceSet"
                webAppMount="/WEB-INF/lib/bcpkix.jar"/>

        <PostResources base="@@DBPS_JAR_DIR@@/databasepropertiesstorage.jar"
                className="org.apache.catalina.webresources.FileResourceSet"
                webAppMount="/WEB-INF/lib/databasepropertiesstorage.jar"/>

        <PostResources base="@@PGSQL_JAR_DIR@@/postgresql-jdbc.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/postgresql-jdbc.jar"/>

        <PostResources base="@@PF_JAR_DIR@@/primefaces.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/primefaces.jar"/>

        <PostResources base="@@MYFACES_JAR_DIR@@/myfaces-api.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/myfaces-api.jar"/>
        <PostResources base="@@MYFACES_JAR_DIR@@/myfaces-impl.jar"
                 className="org.apache.catalina.webresources.FileResourceSet"
                 webAppMount="/WEB-INF/lib/myfaces-impl.jar"/>
    </Resources>
</Context>
